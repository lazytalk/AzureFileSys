{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "FileService": "Information"
    }
  },
  "EnvironmentMode": "Staging",
  "BlobStorage": {
    "UseLocalStub": false,
    "ContainerName": "userfiles-staging",
    "InitialTransferSizeBytes": 8388608,
    "MaximumTransferSizeBytes": 8388608,
    "MaxConcurrency": 16,
    "EnableProgressTracking": true,
    "MaxFileSizeBytes": 1073741824
    ,
    "MaxConcurrentUploads": 16
  },
  "Persistence": {
    "UseInMemory": false,
    "UseTableStorage": true,
    "UseEf": false,
    "UseSqlServer": false,
    "AutoMigrate": false
  },
  "ExternalAuth": {
    "Enabled": false,
    "RequireValidation": true
  },
  "Features": {
    "EnableDetailedErrors": true,
    "EnableSwagger": true,
    "MaxFileSizeMB": 10,
    "EnableCors": true
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Upload": {
    "ApiKey": ""
  },
  "AllowedHosts": "*"
}
