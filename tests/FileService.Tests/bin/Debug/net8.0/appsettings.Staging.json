{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "FileService": "Information"
    }
  },
  "EnvironmentMode": "Staging",
  "BlobStorage": {
    "UseLocalStub": false,
    "ContainerName": "userfiles-staging"
  },
  "Persistence": {
    "UseEf": true,
    "UseSqlServer": true,
    "AutoMigrate": true
  },
  "ExternalAuth": {
    "Enabled": false,
    "RequireValidation": true
  },
  "Features": {
    "EnableDetailedErrors": true,
    "EnableSwagger": true,
    "MaxFileSizeMB": 10,
    "EnableCors": true
  },
  "AllowedHosts": "*"
}
