{
  "BlobStorage": {
    "ConnectionString": "",
    "ContainerName": "userfiles",
    "UseLocalStub": true
  },
  "Persistence": {
    "Type": "InMemory"
  },
  "TableStorage": {
    "TableName": "FileMetadata"
  },
  "PowerSchool": {
    "EnableDevBypass": true,
    "RequireValidation": false,
    "TokenTimeoutMinutes": 30,
    "PluginBaseUrl": "http://localhost:8080",
    "PluginPath": "/powerschool-plugin/FileServiceTools/web_root/admin/FileServiceTools.html",
    "AllowedOrigins": [
      "http://localhost:8080",
      "http://localhost:5090",
      "http://localhost:3000"
    ]
  },
  "EnvironmentMode": "Development", 
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
