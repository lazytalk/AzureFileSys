{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FileService": "Information"
    }
  },
  "EnvironmentMode": "Staging",
  "BlobStorage": {
    "UseLocalStub": false,
    "ContainerName": "userfiles-staging"
  },
  "Persistence": {
    "Type": "TableStorage"
  },
  "TableStorage": {
    "TableName": "FileMetadata"
  },
  "PowerSchool": {
    "EnableDevBypass": false,
    "RequireValidation": true,
    "TokenTimeoutMinutes": 30,
    "PluginBaseUrl": "https://ps1.kaiwenacademy.cn",
    "PluginPath": "/admin/FileServiceTools.html",
    "AllowedOrigins": [
      "https://ps1.kaiwenacademy.cn",
      "https://filesvc-stg-app.kaiweneducation.com"
    ]
  },
  "Features": {
    "EnableDetailedErrors": true,
    "EnableSwagger": true,
    "MaxFileSizeMB": 10,
    "EnableCors": true
  },
  "AllowedHosts": "*"
}
