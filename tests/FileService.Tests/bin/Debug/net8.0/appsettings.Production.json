{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "FileService": "Information"
    }
  },
  "EnvironmentMode": "Production",
  "BlobStorage": {
    "UseLocalStub": false,
    "ContainerName": "userfiles-prod"
  },
  "Persistence": {
    "UseEf": true,
    "UseSqlServer": true,
    "AutoMigrate": false
  },
  "ExternalAuth": {
    "Enabled": false,
    "RequireValidation": true
  },
  "Features": {
    "EnableDetailedErrors": false,
    "EnableSwagger": false,
    "MaxFileSizeMB": 50,
    "EnableCors": false
  },
  "AllowedHosts": "*"
}
