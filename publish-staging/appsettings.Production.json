{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "FileService": "Information"
    }
  },
  "EnvironmentMode": "Production",
  "BlobStorage": {
    "UseLocalStub": false,
    "ContainerName": "userfiles-prod"
  },
  "Persistence": {
    "Type": "TableStorage"
  },
  "TableStorage": {
    "TableName": "FileMetadata"
  },
  "PowerSchool": {
    "EnableDevBypass": false,
    "RequireValidation": true,
    "TokenTimeoutMinutes": 60
  },
  "Features": {
    "EnableDetailedErrors": false,
    "EnableSwagger": false,
    "MaxFileSizeMB": 50,
    "EnableCors": false
  },
  "AllowedHosts": "*"
}
