{
    "version": "0.2.0",
    "configurations": [
              {
            "name": "Build and Launch",
            "type": "coreclr",
            "request": "launch",
                "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/FileService.Api/bin/Debug/net9.0/FileService.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                    "uriFormat": "%s/index.html",
                "action": "openExternally"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5090"
            }
        }
    ]
}