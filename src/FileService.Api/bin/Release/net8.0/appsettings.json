  "Persistence": {
    "UseEf": true,
  "SqlitePath": "files.db",
  "AutoMigrate": true
  },
  
  "Features": {
    "EnableCors": true
  },
  "Cors": {
    "AllowedOrigins": ["*"]
  },
  
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
